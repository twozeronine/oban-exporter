{
    "name": "oban-exporter-dev-container",
    "dockerComposeFile": [
        "docker-compose.yml"
    ],
    "service": "oban-exporter-dev-container",
    "workspaceFolder": "/home/vscode/workspace",
    "customizations": {
        "vscode": {
            "extensions": [
                "jakebecker.elixir-ls",
                "esbenp.prettier-vscode",
                "pgourlain.erlang",
                "royalmist.vscode-eex-format",
                "eamodio.gitlens"
            ]
        }
    },
    "features": {
        "ghcr.io/devcontainers/features/docker-in-docker:2": {
            "version": "latest"
        }
    },
    "remoteUser": "root",
    "shutdownAction": "stopCompose"
}